{
  "schedule": [
    "at 5:00 am every saturday"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch",
      "stabilityDays": 3
    }
  ],
  "branchPrefix": "renovate-",
  "dependencyDashboard": true,
  "ignorePaths": [],
  "prCreation": "not-pending",
  "rangeStrategy": "pin",
  "separateMultipleMajor": true,
  "stabilityDays": 3,
  "vulnerabilityAlerts": {
    "automerge": true,
    "stabilityDays": 0
  }
}
